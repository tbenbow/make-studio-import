{
  "fonts": [
    {
      "family": "Bitcount Single",
      "weight": 400,
      "style": "normal"
    },
    {
      "family": "Instrument Serif",
      "weight": 400,
      "style": "normal"
    },
    {
      "family": "Inter",
      "weight": 400,
      "style": "normal"
    },
    {
      "family": "Inter",
      "weight": 500,
      "style": "normal"
    },
    {
      "family": "Inter",
      "weight": 600,
      "style": "normal"
    },
    {
      "family": "Roboto",
      "weight": 400,
      "style": "normal"
    }
  ],
  "systemColors": {
    "brand": "#ff00f7",
    "brand-hover": "#ff007b",
    "on-brand": "#ffffff",
    "base": "#aaff00",
    "base-muted": "#181816",
    "base-panel": "#1dbf9e",
    "fg": "#ff0000",
    "fg-muted": "#a5a596",
    "fg-subtle": "#7c7c67",
    "border": "#464644"
  },
  "customColors": [],
  "palette": {
    "primary": {
      "label": "primary",
      "colors": [
        "rgb(224, 231, 255)",
        "rgb(165, 180, 251)",
        "rgb(100, 102, 241)",
        "rgb(67, 56, 202)",
        "rgb(48, 46, 129)"
      ]
    },
    "accent1": {
      "label": "accent-1",
      "colors": [
        "rgb(206, 250, 254)",
        "rgb(103, 232, 249)",
        "rgb(7, 182, 212)",
        "rgb(13, 116, 144)",
        "rgb(22, 78, 99)"
      ]
    },
    "accent2": {
      "label": "accent-2",
      "colors": [
        "rgb(209, 250, 229)",
        "rgb(110, 231, 183)",
        "rgb(18, 185, 129)",
        "rgb(4, 120, 87)",
        "rgb(18, 78, 74)"
      ]
    },
    "accent3": {
      "label": "accent-3",
      "colors": [
        "rgb(254, 243, 199)",
        "rgb(251, 211, 76)",
        "rgb(245, 158, 12)",
        "rgb(180, 83, 10)",
        "rgb(120, 53, 15)"
      ]
    },
    "grays": {
      "label": "gray",
      "colors": [
        "rgb(241, 245, 249)",
        "rgb(203, 213, 225)",
        "rgb(100, 116, 139)",
        "rgb(51, 65, 85)",
        "rgb(15, 23, 42)"
      ]
    }
  },
  "headingTypography": {
    "heading-xl": {
      "fontFamily": "Bitcount Single",
      "fontWeight": 400,
      "fontSize": 160,
      "lineHeight": 80,
      "letterSpacing": -2,
      "mobileLineHeight": 48,
      "mobileFontSize": 48
    },
    "heading-lg": {
      "fontFamily": "Bitcount Single",
      "fontWeight": 400,
      "fontSize": 48,
      "lineHeight": 56,
      "letterSpacing": -1.2,
      "mobileLineHeight": 40,
      "mobileFontSize": 32
    },
    "heading-md": {
      "fontFamily": "Bitcount Single",
      "fontWeight": 400,
      "fontSize": 32,
      "lineHeight": 40,
      "letterSpacing": -0.8,
      "mobileLineHeight": 36,
      "mobileFontSize": 28
    },
    "heading-sm": {
      "fontFamily": "Instrument Serif",
      "fontWeight": 400,
      "fontSize": 24,
      "lineHeight": 32,
      "letterSpacing": -0.5,
      "mobileLineHeight": 28,
      "mobileFontSize": 20
    },
    "heading-xs": {
      "fontFamily": "Instrument Serif",
      "fontWeight": 400,
      "fontSize": 18,
      "lineHeight": 28,
      "letterSpacing": -0.3,
      "mobileLineHeight": 24,
      "mobileFontSize": 16
    }
  },
  "bodyTypography": {
    "body-lg": {
      "fontFamily": "Inter",
      "fontWeight": 400,
      "fontSize": 18,
      "lineHeight": 32,
      "letterSpacing": 0
    },
    "body-md": {
      "fontFamily": "Inter",
      "fontWeight": 400,
      "fontSize": 16,
      "lineHeight": 28,
      "letterSpacing": 0
    },
    "body-sm": {
      "fontFamily": "Inter",
      "fontWeight": 400,
      "fontSize": 14,
      "lineHeight": 24,
      "letterSpacing": 0
    }
  },
  "prose": {
    "elements": {
      "h1": {
        "typographyClass": "heading-xl",
        "marginBottom": 1.5
      },
      "h2": {
        "typographyClass": "heading-lg",
        "marginBottom": 1.25
      },
      "h3": {
        "typographyClass": "heading-md",
        "marginBottom": 1
      },
      "h4": {
        "typographyClass": "heading-sm",
        "marginBottom": 0.75
      },
      "h5": {
        "typographyClass": "heading-xs",
        "marginBottom": 0.5
      },
      "h6": {
        "typographyClass": "heading-xs",
        "marginBottom": 0.5
      },
      "p": {
        "typographyClass": "body-md",
        "marginBottom": 1.25
      },
      "ul": {
        "typographyClass": "body-md",
        "marginBottom": 1.25
      },
      "ol": {
        "typographyClass": "body-md",
        "marginBottom": 1.25
      }
    },
    "lists": {
      "listStyleType": "disc",
      "indent": 1.5,
      "itemSpacing": 0.5,
      "nestedIndent": 1.5
    },
    "links": {
      "color": "accent",
      "hoverColor": "primary",
      "underline": "always"
    },
    "customCSS": "blockquote {\n  border-left: 3px solid currentColor;\n  padding-left: 1.5rem;\n  margin: 1.5rem 0;\n  font-style: italic;\n  opacity: 0.85;\n}\n\npre {\n  background: rgba(0, 0, 0, 0.3);\n  padding: 1rem;\n  border-radius: 4px;\n  font-family: 'SF Mono', 'Fira Code', monospace;\n  font-size: 0.9em;\n  overflow-x: auto;\n  white-space: pre-wrap !important;\n  word-wrap: break-word !important;\n}\n\npre code {\n  white-space: pre-wrap !important;\n  word-wrap: break-word !important;\n}\n\ncode:not(pre code) {\n  background: rgba(0, 0, 0, 0.2);\n  padding: 0.15em 0.4em;\n  border-radius: 3px;\n  font-family: 'SF Mono', 'Fira Code', monospace;\n  font-size: 0.9em;\n}"
  },
  "customCSS": ".prose {\n  color: #a5a596;\n}\n.prose h1,\n.prose h2, \n.prose h3, \n.prose h4 {\n  color: white;\n}"
}
