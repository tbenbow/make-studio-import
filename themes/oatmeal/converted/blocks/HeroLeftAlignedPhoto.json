{
  "makeStudioFields": true,
  "version": 1,
  "description": "Left-aligned with photo below",
  "fields": [
    {
      "type": "text",
      "name": "Eyebrow",
      "default": "Welcome"
    },
    {
      "type": "text",
      "name": "Headline",
      "default": "Ship faster with confidence."
    },
    {
      "type": "wysiwyg",
      "name": "Subheadline",
      "default": "<p>Build, test, and deploy with tools designed to help your team move quickly without breaking things.</p>"
    },
    {
      "type": "image",
      "name": "Photo",
      "default": "https://images.unsplash.com/photo-1551434678-e076c223a692?w=1200&h=800&fit=crop",
      "config": {
        "maxWidth": 1920,
        "maxHeight": 1080
      }
    },
    {
      "type": "items",
      "name": "Buttons",
      "default": [
        { "label": "Get started", "link": "#", "style": "primary" },
        { "label": "See pricing", "link": "#", "style": "ghost" }
      ],
      "config": {
        "fields": [
          { "type": "text", "name": "Label" },
          { "type": "text", "name": "Link" },
          {
            "type": "select",
            "name": "Style",
            "config": {
              "selectOptions": [
                { "key": "Primary", "value": "primary" },
                { "key": "Secondary", "value": "secondary" },
                { "key": "Ghost", "value": "ghost" }
              ]
            }
          }
        ]
      }
    }
  ]
}
