{
  "fonts": [
    { "family": "Inter", "weight": 400, "style": "normal" },
    { "family": "Inter", "weight": 500, "style": "normal" },
    { "family": "Inter", "weight": 600, "style": "normal" },
    { "family": "Inter", "weight": 700, "style": "normal" }
  ],
  "systemColors": {
    "brand": "#14b8a6",
    "on-brand": "#ffffff",
    "base": "#fafafa",
    "base-muted": "#f4f4f5",
    "base-alt": "#0d9488",
    "panel": "#ffffff",
    "fg": "#27272a",
    "fg-muted": "#52525b",
    "fg-alt": "#a1a1aa",
    "border": "#e4e4e7"
  },
  "customColors": [],
  "palette": {
    "primary": {
      "label": "primary",
      "colors": ["#ccfbf1", "#5eead4", "#14b8a6", "#0d9488", "#134e4a"]
    },
    "accent1": {
      "label": "accent-1",
      "colors": ["#cffafe", "#67e8f9", "#06b6d4", "#0e7490", "#164e63"]
    },
    "accent2": {
      "label": "accent-2",
      "colors": ["#d1fae5", "#6ee7b7", "#10b981", "#047857", "#064e3b"]
    },
    "accent3": {
      "label": "accent-3",
      "colors": ["#fef3c7", "#fcd34d", "#f59e0b", "#b45309", "#78350f"]
    },
    "grays": {
      "label": "gray",
      "colors": ["#f4f4f5", "#d4d4d8", "#71717a", "#3f3f46", "#18181b"]
    }
  },
  "headingTypography": {
    "heading-xl": {
      "fontFamily": "Inter",
      "fontWeight": 700,
      "fontSize": 48,
      "lineHeight": 56,
      "letterSpacing": -1.2,
      "mobileFontSize": 32,
      "mobileLineHeight": 40
    },
    "heading-lg": {
      "fontFamily": "Inter",
      "fontWeight": 700,
      "fontSize": 30,
      "lineHeight": 36,
      "letterSpacing": -0.8,
      "mobileFontSize": 24,
      "mobileLineHeight": 32
    },
    "heading-md": {
      "fontFamily": "Inter",
      "fontWeight": 600,
      "fontSize": 24,
      "lineHeight": 32,
      "letterSpacing": -0.5,
      "mobileFontSize": 20,
      "mobileLineHeight": 28
    },
    "heading-sm": {
      "fontFamily": "Inter",
      "fontWeight": 600,
      "fontSize": 16,
      "lineHeight": 24,
      "letterSpacing": -0.2,
      "mobileFontSize": 16,
      "mobileLineHeight": 24
    },
    "heading-xs": {
      "fontFamily": "Inter",
      "fontWeight": 600,
      "fontSize": 14,
      "lineHeight": 20,
      "letterSpacing": 0,
      "mobileFontSize": 14,
      "mobileLineHeight": 20
    }
  },
  "bodyTypography": {
    "body-lg": {
      "fontFamily": "Inter",
      "fontWeight": 400,
      "fontSize": 18,
      "lineHeight": 28,
      "letterSpacing": 0
    },
    "body-md": {
      "fontFamily": "Inter",
      "fontWeight": 400,
      "fontSize": 16,
      "lineHeight": 28,
      "letterSpacing": 0
    },
    "body-sm": {
      "fontFamily": "Inter",
      "fontWeight": 400,
      "fontSize": 14,
      "lineHeight": 24,
      "letterSpacing": 0
    }
  },
  "prose": {
    "elements": {
      "h1": { "typographyClass": "heading-xl", "marginBottom": 1.5 },
      "h2": { "typographyClass": "heading-lg", "marginBottom": 1.25 },
      "h3": { "typographyClass": "heading-md", "marginBottom": 1 },
      "h4": { "typographyClass": "heading-sm", "marginBottom": 0.75 },
      "h5": { "typographyClass": "heading-xs", "marginBottom": 0.5 },
      "h6": { "typographyClass": "heading-xs", "marginBottom": 0.5 },
      "p": { "typographyClass": "body-md", "marginBottom": 1.25 },
      "ul": { "typographyClass": "body-md", "marginBottom": 1.25 },
      "ol": { "typographyClass": "body-md", "marginBottom": 1.25 }
    },
    "lists": {
      "listStyleType": "disc",
      "indent": 1.5,
      "itemSpacing": 0.5,
      "nestedIndent": 1.5
    },
    "links": {
      "color": "accent",
      "hoverColor": "primary",
      "underline": "always"
    },
    "customCSS": "blockquote {\n  border-left: 3px solid #d4d4d8;\n  padding-left: 1.5rem;\n  margin: 1.5rem 0;\n  font-style: italic;\n  color: #52525b;\n}\n\npre {\n  background: #f4f4f5;\n  padding: 1rem;\n  border-radius: 8px;\n  font-family: 'SF Mono', 'Fira Code', monospace;\n  font-size: 0.9em;\n  overflow-x: auto;\n  white-space: pre-wrap !important;\n  word-wrap: break-word !important;\n  border: 1px solid #e4e4e7;\n}\n\npre code {\n  white-space: pre-wrap !important;\n  word-wrap: break-word !important;\n}\n\ncode:not(pre code) {\n  background: #f4f4f5;\n  padding: 0.15em 0.4em;\n  border-radius: 3px;\n  font-family: 'SF Mono', 'Fira Code', monospace;\n  font-size: 0.9em;\n}"
  },
  "customCSS": ".prose {\n  color: #52525b;\n}\n.prose h1,\n.prose h2,\n.prose h3,\n.prose h4 {\n  color: #27272a;\n}"
}
