      "name": "Button",
      "site_id": "6994aa51d89f2ba4716738fa",
      "template": "{{#switch style}}\n  {{#case \"primary\"}}\n    <a href=\"{{link}}\" class=\"btn btn-md btn-primary\">\n      {{default label \"Click Here\"}}\n    </a>\n  {{/case}}\n  {{#case \"secondary\"}}\n    <a href=\"{{link}}\" class=\"btn btn-md btn-secondary\">\n      {{default label \"Click Here\"}}\n    </a>\n  {{/case}}\n  {{#case \"outline\"}}\n    <a href=\"{{link}}\" class=\"btn btn-md btn-outline\">\n      {{default label \"Click Here\"}}\n    </a>\n  {{/case}}\n  {{#case \"ghost\"}}\n    <a href=\"{{link}}\" class=\"btn btn-md btn-ghost\">\n      {{default label \"Click Here\"}}\n    </a>\n  {{/case}}\n  {{#otherwise}}\n    <a href=\"{{link}}\" class=\"btn btn-md btn-primary\">\n      {{default label \"Click Here\"}}\n    </a>\n  {{/otherwise}}\n{{/switch}}\n",
      "__v": 0,
